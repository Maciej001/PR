<!-- Side -->
<td> 
	<% if @side == 'bid': %>
		<i class='fi-arrow-up'></i>
	<% else: %>
		<i class='fi-arrow-down'></i>
	<% end %>
	<%= @side %>
</td>

<!-- Status -->
<td>
	<%= @state %>
</td>

<!-- Price -->
<td><%= Number(@price).toFixed(0) %></td>

<!-- Size -->
<td><%= Number(@size).toFixed(0) %></td>

<!-- Updated -->
<td><%= moment(@updated_at).format('D MMM, H:MM') %></td>

<!-- Actions -->
<td>
	<ul>
		<li class="button tiny alert delete-button">Delete</li>
	</ul>
</td>
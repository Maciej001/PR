<!-- Side -->
<td>
	<% if status == '0' %>
		"Buy"
	<% else %>
		"Sell"
	<% end %>
</td>

<!-- Status -->
<td>
	<% if state == '0' %>
		"Live"
	<% else if state == '1' %>
		"Cancelled"
	<% else %>
		"Executed"
	<% end %>
</td>

<!-- Price -->
<td><%= price %></td>

<!-- Size -->
<td><%= size %></td>

<!-- Created -->
<td><%= created_at %></td>

<!-- Updated -->
<td><%= updated_at %></td>

<!-- Actions -->
<td>
	<ul>
		<li class="button">Update</li>
		<li class="button">Delete</li>
	</ul>
</td>